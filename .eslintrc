{
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:jest/recommended"
  ],
  "plugins": ["jest"],
  "rules": {
    "semi": [2, "never"],
    "object-shorthand": 2,
    "prefer-destructuring": 2,
    "no-console": 1,
    "react/prop-types": 0
  }
}
